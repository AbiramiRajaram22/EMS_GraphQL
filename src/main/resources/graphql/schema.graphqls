# Root Queries
type Query {
    employees: [Employee]!
    employee(empName: String!): Employee!
}

type Employee {
    elementId: ID
    empId : String
    empName : String
    dateOfHire : String
    mailId : String
    prevExp : Float
    triconExp : Float
    totalExp : Float
    billingStatus : String
    remarks : String
    leaderPipeline : String
    businessAccumen : String
    coachable : String
}